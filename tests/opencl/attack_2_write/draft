__kernel void covertListener(__global volatile int *A, __global volatile int *B, __global volatile int *C) {
  local volatile int lm[SHARED_MEMORY_SIZE_INT];
  
  for (int i = get_local_id(0); i < SHARED_MEMORY_SIZE_TRAVERSED; i+= get_local_size(0)) {
    C[((SHARED_MEMORY_SIZE_TRAVERSED * get_group_id(0)) + i)] = lm[i];
  }
    
}